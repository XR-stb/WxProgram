/* pages/login/login.wxss */
.container{
    height: 100vh;
    display: flex;
    flex-direction: column;
    padding: 30rpx;
    background: rgb(241,242,246);
}

.info{
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    font-size:40rpx;
}

.info image{
    border-radius: 50%;
}

.info text{
    margin-left: 30rpx;
}

image{
    width: 100rpx;
    height: 100rpx;
}

.func{
    display: flex;
    flex-direction: column;
    height: 300rpx;
    background: white;
    margin-top: 40rpx;
    padding-top: 30rpx;
    padding-left: 20rpx;
    justify-content: center;
    border-radius: 20rpx;
}

.title{
    font-size: 35rpx;
    color: rgb(50, 53, 54);
}

.mybox{
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 200rpx;
    margin-top: 30rpx;
    background: white;
}

.entity{
    width: 100%;
}

.myfont{
    font-size: 30rpx;
}